{
  "CommonConfig": {
    "DataPath": "data",
    "MusicPath": "music",
    "SharedSecret": "FromUserSecrets"
  },
  "Modules": {
    "ModulesFolder": "modules"
  },
  "TagReader": {
    "AlbumTitleArticleNormalization": true,
    "NormalizationArticles": [ "The", "A", "An" ],
    "TagMappings": {
      "AlbumArtist" : 1,
      "Artist" : 2,
      "Composer" : 3
    }
  },
  "OpenIdConnect": {
    "Authority": "FromUserSecrets",
    "ClientId": "FromUserSecrets",
    "ClientSecret": "FromUserSecrets",
    "AdditionalScopes": "",
    "SupportsEndSession": false,
    "AdminClaimKey": "FromUserSecrets",
    "AdminClaimValue": "FromUserSecrets",
    "UsernameClaimKey": "FromUserSecrets",
    "UseOidc": true
  },
  "Database": {
    "Type": "sqlite"
  },
  "ConnectionStrings": {
    "SegnoSharpDatabaseSqlite": "Data Source=SegnoSharp.db",
    "SegnoSharpDatabaseMysql": "FromUserSecrets"
  }
}
